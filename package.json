{
  "name": "vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "organize-images:watch": "node scripts/organize-images.mjs --watch",
    "rename-images-by-exif": "node scripts/rename-images-by-exif.mjs",
    "organize-to-public": "node scripts/organize-to-public.mjs",
    "generate-data-from-public": "node scripts/generate-data-from-public.mjs",
    "compress-webp": "node scripts/compress-webp.mjs",
    "organize-old": "npm run rename-images-by-exif && npm run organize-to-public && npm run generate-data-from-public",
    "organize-source": "npm run organize-to-public && npm run generate-data-from-public"
  },
  "dependencies": {
    "@vueuse/core": "^13.3.0",
    "exifr": "^7.1.3",
    "imagemin": "^9.0.1",
    "imagemin-webp": "^8.0.0",
    "lodash": "^4.17.21",
    "remixicon": "^4.6.0",
    "vue": "^3.5.13",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.17",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^10.2.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.8"
  }
}
